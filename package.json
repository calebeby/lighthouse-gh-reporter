{
  "name": "lighthouse-gh-reporter",
  "version": "1.2.0",
  "description": "",
  "bin": {
    "lighthouse-github-reporter": "./bin/lighthouse-github-reporter.js"
  },
  "scripts": {
    "lib:js": "npx rimraf ./lib && npx mkdirp ./lib && babel --presets sui ./src --out-dir ./lib",
    "lib": "npm run lib:js",
    "prepublish": "npm run lib"
  },
  "config": {
    "sui-ssr": {
      "forceWWW": "true"
    },
    "validate-commit-msg": {
      "types": "@s-ui/mono/src/types"
    },
    "lighthouse-github-reporter": {
      "urls": [
        "http://www.milanuncios.com",
        "http://www.milanuncios.com/legal/indice"
      ],
      "apiUrl": "https://github.schibsted.io/api/v3",
      "owner": "scmspain",
      "repository": "frontend-ma--web-app",
      "strictReview": false,
      "scoreThresholds": {
        "performance": 100,
        "pwa": 40,
        "accessibility": 30,
        "best-practices": 100,
        "seo": 100
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "15.11.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.26.0",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-sui": "1",
    "babelify": "8.0.0",
    "chalk": "2.3.0",
    "colors": "^1.3.2",
    "commander": "2.12.1",
    "lighthouse": "^3.1.1"
  }
}
